{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "remote-mcp-server",
	"main": "src/index.ts",
	"compatibility_date": "2025-03-10",
	"migrations": [
		{
			"tag": "v1",
			"new_classes": ["MyMCP"]
		}
	],
	"durable_objects": {
		"bindings": [
			{
				"class_name": "MyMCP",
				"name": "MCP_OBJECT",
				"script_name": "remote-mcp-server"
			}
		]
	},
	"kv_namespaces": [
		{
			"binding": "OAUTH_KV",
			"id": "6b507675547c43149c0c9e45116a41ba"
		}
	],
	"observability": {
		"enabled": true
	},
	"assets": { 
		"directory": "./static/", 
		"binding": "ASSETS" 
	}
}
